<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>Controlador</servlet-name>
        <servlet-class>Ciclo3.Proyecto.Controlador.Controlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccesoLogin</servlet-name>
        <servlet-class>Ciclo3.Proyecto.Controlador.AccesoLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorC</servlet-name>
        <servlet-class>Ciclo3.Proyecto.Controlador.ControladorC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorP</servlet-name>
        <servlet-class>Ciclo3.Proyecto.Controlador.ControladorP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorCSV</servlet-name>
        <servlet-class>Ciclo3.Proyecto.Controlador.ControladorCSV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorV</servlet-name>
        <servlet-class>Ciclo3.Proyecto.Controlador.ControladorV</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controlador</servlet-name>
        <url-pattern>/Controlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccesoLogin</servlet-name>
        <url-pattern>/AccesoLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorC</servlet-name>
        <url-pattern>/ControladorC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorP</servlet-name>
        <url-pattern>/ControladorP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorCSV</servlet-name>
        <url-pattern>/ControladorCSV</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorV</servlet-name>
        <url-pattern>/ControladorV</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
